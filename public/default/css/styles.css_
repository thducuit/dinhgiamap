}*/

.footer_col_inner div {
    width: 120px
}
#footer .nav_social {
     padding: 0; 
}

#footer .footer_col {
    min-height: 50px;
    padding: 5px 15px;
}
@media screen and (max-width: 1100px) {
    #logo {
        width: 149px;
        height: 36px;
    }
}
